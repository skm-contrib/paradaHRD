{
    "version": 2,
    "functions": {
        "api/**/*": {
            "runtime": "vercel-php@0.5.2",
            "excludeFiles": "{.gitignore, public/**, node_modules/**/*, vendor/**/*,.vercel/**/*}"
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ]
}
