{
    "version": 2,
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "builds": [
        {
            "config": {
                "serverFiles": [
                    "package.json",
                    "api/**"
                ],
                "tscOptions": {
                    "sourceMap": false
                },
                "internalServer": true
            }
        }
    ]
}
